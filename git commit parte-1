def dados_crescente(lista_cpu):
    return sorted(lista_cpu)

matriz_cpu = [
    [50, 80, 30, 60],
    [10, 20, 40, 90],
    [70, 60, 10, 30],
    [20, 50, 90, 40]
]

for i, regiao in enumerate(matriz_cpu):
    print(f"Região {i+1} (antes): {regiao}")
    print(f"Região {i+1} (ordenada): {dados_crescente(regiao)}\n")
